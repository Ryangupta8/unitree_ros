<?xml version="1.0"?>
<launch>
  <!-- puck launch -->
    <include file="$(find a1_autonomy)/launch/velodyne.launch"/>


    <!-- depthimage_to_laserscan -->j
  <include file="$(find depthimage_to_laserscan)/launch/depth2scan.launch"/>   
  <!-- ar track alvar -->
    <include file="$(find ar_track_alvar)/launch/a1_artag.launch"/>

    <arg name="min_height" default="-0.25" />
    <arg name="max_height" default="0.1" />
   <arg name="launch_nodelet_manager" default="true" />
 
     <!-- pointcloud_to_laserscan -->
       <node pkg="pointcloud_to_laserscan" name="pointcloud_to_laserscan_node" type="pointcloud_to_laserscan_node" >
            <remap from="cloud_in" to="velodyne_points"/>
            <remap from="scan" to="cloud2scan" /><!-- /scan /cloud2scan --> 
            <param name="min_height" value="-0.25"/>
            <param name="max_height" value="0.1"/>
            <param name="angle_min" value="-3.1416"/>
            <param name="angle_max" value="3.1416"/>
            <param name="use_inf" value="false"/>
       </node>

    
</launch>
