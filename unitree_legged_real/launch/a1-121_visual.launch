<?xml version="1.0"?>
<launch>
  <!-- puck launch -->
  <!--  <include file="$(find a1_autonomy)/launch/velodyne.launch"/> -->
  <include file="$(find velodyne_pointcloud)/launch/VLP16_points.launch"/>

   <!--<node name="base_to_velodyne" pkg="tf" type="static_transform_publisher" args="0.0075 0.0 0.3025 0 0 0 base velodyne 100"/> -->
