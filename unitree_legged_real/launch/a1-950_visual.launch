<?xml version="1.0"?>
<launch>
  <!-- puck launch -->
  <include file="$(find urg_node)/launch/urg_lidar.launch"/>

  <node name="base_to_camera" pkg="tf" type="static_transform_publisher" args="0.3 0 0 0 0 0 base camera_depth_frame 100"/>


  <arg name="launch_nodelet_manager" default="true" />
 
  <include file="$(find unitree_legged_real)/launch/realsense_to_laserscan.launch"/>

  <param name="robot_description" command="$(find xacro)/xacro '$(find a1_description)/xacro/robot.xacro' DEBUG:=false"/>
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />
</launch>
